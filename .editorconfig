# http://editorconfig.org
root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space

[*.go]
indent_style = tab
indent_size = 4

[*.tmpl]
indent_style = tab
indent_size = 2

[*.{yaml, yml}]
indent_size = 2

[*.js]
indent_size = 2

[*.sql]
indent_size = 4
